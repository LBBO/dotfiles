{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "leading_diamond": "\ue0c7\u2593",
          "foreground": "#2f2f2f",
          "background": "#CCCCCC",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "foreground": "#CCCCCC",
          "background": "#CCCCCC",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf0e7"
          },
          "foreground": "#ffff66",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": " <#B5B2C2>\ue5fe </>",
            "home_icon": "\uf015 "
          },
          "foreground": "#fafafa",
          "powerline_symbol": "\ue0b0",
          "background": "#0037DA",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "ahead_color": "#2EC4B6",
            "behind_color": "#8A4FFF",
            "cache_duration": "none",
            "color_background": false,
            "display_stash_count": true,
            "display_status": true,
            "display_upstream_icon": true,
            "local_changes_color": "#ffeb3b",
            "staging_color": "#2FDA4E",
            "status_colors_enabled": true,
            "working_color": "#E84855"
          },
          "foreground": "#ffeb3b",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "paused_icon": "\uf8e3 ",
            "playing_icon": "\ue602 ",
            "prefix": "\uf9c6 ",
            "stopped_icon": "\uf04d ",
            "track_separator": " - "
          },
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#1BD760",
          "type": "spotify",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "leading_diamond": "\ue0b2",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue718 "
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#7a7a7a> \ue0b3</>",
          "background": "#2f2f2f",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue235 "
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#7a7a7a> \ue0b3</>",
          "background": "#2f2f2f",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue77f "
          },
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#7a7a7a> \ue0b3</>",
          "background": "#2f2f2f",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "<#007ACC> \uf64f </>",
            "time_format": "15:04:05"
          },
          "trailing_diamond": "\u2593\ue0c6",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "text": "~#@❯"
          },
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "text": "❮"
          },
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "<#fafafa> \uf252 </>",
            "style": "roundrock"
          },
          "foreground": "#FFCE5C",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "color_background": false,
            "error_color": "#E84855",
            "prefix": " \uf705 "
          },
          "foreground": "#9FD356",
          "type": "exit",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
